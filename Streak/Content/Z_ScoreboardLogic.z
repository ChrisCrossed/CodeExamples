//
    function InitializeScoreboard(scoreboardType : Enum_ScoreboardType, playerScore : Integer)
    {
        this.PlayerScore = playerScore;
        this.DataCog = Zero.ObjectStore.RestoreOrArchetype("SaveFile", "SavedData", this.Space);
        
        this.CurrentMenu = scoreboardType;
        this.PreviousMenu = scoreboardType;
        
        this.ScoreboardState = Enum_ScoreboardState.Initialize;
        
        // Load the SpriteText objects
        this.LoadLevelCogs();
        
        // If the file exists, load that score array into a variable
        this.LoadScoreboard();
        
        this.LoadScoreArray(scoreboardType);
        
        this.HighScorePosition = this.CheckScoreboard(playerScore, this.ScoreArray);
        
        Console.WriteLine("Greater than position `this.HighScorePosition`");
        
        // if we returned a value, then we need to begin shifting down all other scores and place the players score upon the board
        if(this.HighScorePosition != -1)
        {
            this.ScoreboardState = Enum_ScoreboardState.StartNameInput;
            this.R_Timer = 0;
            
            var highScoreSound = this.Space.Create(Archetype.Find("SFX_HighScore"));
            highScoreSound.SoundEmitter.Volume = 0.75;
            highScoreSound.SoundEmitter.Play();
            
            // Begin shifting down score positions
            for(var i = 9; i > this.HighScorePosition; --i)
            {
                this.ScoreArray.Set(i, this.ScoreArray.Get(i - 1));
                this.NameArray.Set(i, this.NameArray.Get(i - 1));
            }
            
            // Call a function to record the players name. Return the value and apply that within the ScoreArray.
            this.ScoreArray.Set(this.HighScorePosition, this.PlayerScore);
        }
        
        this.SaveScoreArray(scoreboardType);
        
        // Save the scoreboard.
        this.SaveScoreboard();
        
        this.LoadScoreArray(scoreboardType);
        this.LoadScoreboard();
        
        this.CreateScoreboard();
        
        this.MusicObject = this.Space.Create(Archetype.Find("Music_InventionForStrings"));
        this.MusicObject.SoundEmitter.Play();
        
        Zero.Connect(this.Space, Events.LogicUpdate, this.OnLogicUpdate);
        Zero.Connect(this.GameSession, Events.GameRequestQuit, this.OnGameRequestQuit);
    }
//
    function OnTextTyped(event : KeyboardTextEvent)
    {
        if(event.Character == 8) // Backspace
        {
            if(this.PlayerName.Count > 0)
            {
                this.PlayerName = this.PlayerName.SubString(0, this.PlayerName.Count - 1);
            }
        }
        else if(event.Character == 32) // Spacebar
        {
            
        }
        else if(event.Character == 13 || event.Character == 27) // Enter or ESC
        {
            this.ScoreboardState = Enum_ScoreboardState.EndNameInput;
            this.R_Timer = 0;
        }
        else if(this.PlayerName.Count < 10)
        {
            this.PlayerName = "`this.PlayerName``String.FromChar(event.Character)`";
        }
        
        this.Sprite_NameEntry_Text.SpriteText.Text = "`this.PlayerName`";
    }
//
    function OnLogicUpdate(event : UpdateEvent)
    {
        this.R_Timer += event.Dt;
        
        if(this.ScoreboardState == Enum_ScoreboardState.Initialize)
        {
            this.ChangeScoreboardTitle(this.CurrentMenu);
            this.FadeInText();
            
            if(this.R_Timer > 10)
            {
                Console.WriteLine(this.CurrentMenu);
                this.ScoreboardState = Enum_ScoreboardState.FadeOut;
                this.R_Timer = 0;
            }
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.StartNameInput)
        {
            this.PlayerName = "";
            
            var seq = Actions.Sequence(this.Owner.Actions);
            var group = Actions.Group(seq);
            
            var visible = Real4(1, 1, 1, 1);
            
            Actions.Property(group, @this.Sprite_NameEntry.Sprite.Color, visible, 0.3, Ease.QuadOut);
            Actions.Property(group, @this.Sprite_NameEntry_Background.Sprite.Color, visible, 0.3, Ease.QuadOut);
            Actions.Property(group, @this.Sprite_NameEntry_Text.SpriteText.Color, visible, 0.3, Ease.QuadOut);
            
            if(this.R_Timer > 0.3)
            {
                Zero.Connect(Zero.Keyboard, Events.TextTyped, this.OnTextTyped);
                this.ScoreboardState = Enum_ScoreboardState.NameInput;
                this.R_Timer = 0;
            }
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.NameInput)
        {
            
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.EndNameInput)
        {
            var seq = Actions.Sequence(this.Owner.Actions);
            var group = Actions.Group(seq);
            
            var invisible = Real4(0, 0, 0, 0);
            
            Actions.Property(group, @this.Sprite_NameEntry.Sprite.Color, invisible, 0.3, Ease.QuadOut);
            Actions.Property(group, @this.Sprite_NameEntry_Background.Sprite.Color, invisible, 0.3, Ease.QuadOut);
            Actions.Property(group, @this.Sprite_NameEntry_Text.SpriteText.Color, invisible, 0.3, Ease.QuadOut);
            
            Console.WriteLine("Applying `this.PlayerName`");
            this.NameArray.Set(this.HighScorePosition, this.PlayerName);
            
            this.SaveScoreArray(this.CurrentMenu);
        
            // Save the scoreboard.
            this.SaveScoreboard();
            
            this.LoadScoreArray(this.CurrentMenu);
            this.LoadScoreboard();
            
            this.CreateScoreboard();
            
            Zero.Disconnect(Zero.Keyboard, Events.TextTyped, this);
            this.ScoreboardState = Enum_ScoreboardState.Initialize;
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.FadeOut)
        {
            this.FadeOutText(0.5);
            if(this.R_Timer > 1)
            {
                this.ScoreboardState = Enum_ScoreboardState.PickNew;
                this.R_Timer = 0;
            }
            
            if( Zero.Keyboard.KeyIsPressed(Keys.Enter) || Zero.Keyboard.KeyIsPressed(Keys.Space) )
            {
                this.ScoreboardState = Enum_ScoreboardState.Quit;
            }
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.PickNew)
        {
            this.R_Timer = 0;
            this.PreviousMenu = this.CurrentMenu;
            this.CallRandomScoreboard();
            
            this.ChangeScoreboardTitle(this.CurrentMenu);
            
            this.LoadScoreArray(this.CurrentMenu);
            Console.WriteLine(this.CurrentMenu);
            this.CreateScoreboard();
            
            this.ScoreboardState = Enum_ScoreboardState.FadeIn;
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.FadeIn)
        {
            this.FadeInText();
            if(this.R_Timer > 10)
            {
                this.ScoreboardState = Enum_ScoreboardState.FadeOut;
                this.R_Timer = 0;
            }
            
            if( Zero.Keyboard.KeyIsPressed(Keys.Enter) || Zero.Keyboard.KeyIsPressed(Keys.Space) )
            {
                this.ScoreboardState = Enum_ScoreboardState.Quit;
            }
        }
        else if(this.ScoreboardState == Enum_ScoreboardState.Quit)
        {
            this.FadeOutText(2.0);
            
            
            this.MusicObject.SoundEmitter.Volume -= (event.Dt / 2.5); // Spanning out the volume fade by 2.5 seconds
            
            if(this.R_Timer > 3)
            {
                while(this.GameSession.FindSpaceByName("LevelSpace") != null)
                {
                    this.GameSession.FindSpaceByName("LevelSpace").Destroy();
                }
                
                while(this.GameSession.FindSpaceByName("ThemeSpace") != null)
                {
                    this.GameSession.FindSpaceByName("ThemeSpace").Destroy();
                }
                
                while(this.GameSession.FindSpaceByName("HudSpace") != null)
                {
                    this.GameSession.FindSpaceByName("HudSpace").Destroy();
                }
                
                this.GameSession.CreateNamedSpace("Main", Archetype.Find("Space"));
                this.GameSession.FindSpaceByName("Main").LoadLevel(Level.Find("Menu_MainMenu"));
            }
        }
    }
