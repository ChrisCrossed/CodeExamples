//
    function CallRandomScoreboard()
    {
        do
        {
            var dieRoll = this.Rand.DieRoll(12);
            
            if(dieRoll == 1)
            {
                this.CurrentMenu = Enum_ScoreboardType.Normal_5;
            }
            else if(dieRoll == 2)
            {
                this.CurrentMenu = Enum_ScoreboardType.Normal_10;
            }
            else if(dieRoll == 3)
            {
                this.CurrentMenu = Enum_ScoreboardType.Normal_20;
            }
            else if(dieRoll == 4)
            {
                this.CurrentMenu = Enum_ScoreboardType.Normal_Unlimited;
            }
            else if(dieRoll == 5)
            {
                this.CurrentMenu = Enum_ScoreboardType.Hard_5;
            }
            else if(dieRoll == 6)
            {
                this.CurrentMenu = Enum_ScoreboardType.Hard_10;
            }
            else if(dieRoll == 7)
            {
                this.CurrentMenu = Enum_ScoreboardType.Hard_20;
            }
            else if(dieRoll == 8)
            {
                this.CurrentMenu = Enum_ScoreboardType.Hard_Unlimited;
            }
            else if(dieRoll == 9)
            {
                this.CurrentMenu = Enum_ScoreboardType.Extreme_5;
            }
            else if(dieRoll == 10)
            {
                this.CurrentMenu = Enum_ScoreboardType.Extreme_10;
            }
            else if(dieRoll == 11)
            {
                this.CurrentMenu = Enum_ScoreboardType.Extreme_20;
            }
            else if(dieRoll == 12)
            {
                this.CurrentMenu = Enum_ScoreboardType.Extreme_Unlimited;
            }
            this.R_Timer = 0;
        } while(this.CurrentMenu == this.PreviousMenu)
    }
