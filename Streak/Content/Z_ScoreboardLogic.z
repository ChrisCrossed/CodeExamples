    function LoadLevelCogs()
    {
        this.ScoreboardName = this.Space.FindObjectByName("Text_GameMode");
        this.ScoreboardTitle = this.Space.FindObjectByName("Text_HighScores");
        
        this.Sprite_NameEntry = this.Space.FindObjectByName("Sprite_NameEntry");
        this.Sprite_NameEntry_Background = this.Space.FindObjectByName("Sprite_NameEntry_Background");
        this.Sprite_NameEntry_Text = this.Space.FindObjectByName("Sprite_NameEntry_Text");
        
