    function CheckScoreboard(playerScore : Integer, scoreArray : Array[Integer]):Integer
    {
        // We start at the 1st place (0) and work our way to 10th place (9)
        for(var i = 0; i < 10; ++i)
        {
            Console.WriteLine("Checking `playerScore` against `scoreArray.Get(i)`");
            if(playerScore > scoreArray.Get(i))
            {
                return i;
            }
        }
        
        return -1;
    }
